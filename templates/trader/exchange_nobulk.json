{
    "query": {
        "status": {
            "option": "online"
        },
        "name": "{{item_name}}",
        "type": "{{item_name_type}}",
        "filters": {
            "trade_filters": {
                "disabled": false,
                "sale_type": {
                    "option": "priced"
                },
                "filters": {
                    "price": {
                        "min": {{price_min}},
                        "max": {{price_max}}
                    }
                }
            }
        }
    },
    "sort": {
        "price": "asc"
    }
}
